{
    "manifest_version": 3,
    "name": "Auto Form Filler",
    "version": "1.1",
    "description": "Store custom details and fill forms with one click.",
    "permissions": ["storage", "tabs"],
    "options_page": "options.html",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/go.svg",
        "48": "icons/go.svg",
        "128": "icons/go.svg"
      }
    },
    "icons": {
      "16": "icons/go.svg",
      "48": "icons/go.svg",
      "128": "icons/go.svg"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content_script.js"],
        "css": ["style.css"]
      }
    ]
  }
  